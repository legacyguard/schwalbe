# Supabase - Required for database and authentication
VITE_SUPABASE_URL="YOUR_SUPABASE_URL_HERE"
VITE_SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_KEY_HERE"

# API URL - The backend URL for your application
VITE_API_URL="http://localhost:3000"

# Google Analytics - Recommended for tracking application usage
VITE_GA_TRACKING_ID="G-XXXXXXXXXX"

# Sentry - Recommended for error monitoring
VITE_SENTRY_DSN=""

# Stripe - Recommended for payments
VITE_STRIPE_PUBLISHABLE_KEY=""

# Sofia AI Assistant - Optional feature
VITE_SOFIA_AI_BASE_URL=""
VITE_SOFIA_AI_API_KEY=""

# Feature Flags - Use 1 to enable, 0 to disable
VITE_ENABLE_HOLLYWOOD_LANDING="0"
VITE_ENABLE_DASHBOARD_V2="1"
VITE_ENABLE_ASSISTANT="1"
VITE_ENABLE_ONBOARDING="1"
