{
  "name": "@schwalbe/shared",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index-minimal.js",
  "types": "./dist/index-minimal.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index-minimal.js",
      "types": "./dist/index-minimal.d.ts"
    },
    "./search": {
      "import": "./dist/search/index.js",
      "types": "./dist/search/index.d.ts"
    },
    "./lib/logger": {
      "import": "./dist/lib/logger.js",
      "types": "./dist/lib/logger.d.ts"
    },
    "./lib/search-service": {
      "import": "./dist/lib/search-service.js",
      "types": "./dist/lib/search-service.d.ts"
    },
    "./lib/resend": {
      "import": "./dist/lib/resend.js",
      "types": "./dist/lib/resend.d.ts"
    },
    "./types/reminders": {
      "import": "./dist/types/reminders.js",
      "types": "./dist/types/reminders.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:ci": "eslint src/ || true",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@schwalbe/logic": "*",
    "@supabase/supabase-js": "^2.57.4",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.5",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "~18.3.12",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.3.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  }
}