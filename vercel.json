{
  "version": 2,
  "buildCommand": "npm ci && npm run build --workspace=@schwalbe/web",
  "outputDirectory": "apps/web/dist",
  "installCommand": "echo 'Skip install - using npm ci in build'",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}